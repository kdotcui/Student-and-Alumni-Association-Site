<h1>Event Data for <%= event.title %></h1>
<table class="table">
  <tr>
    <th>Title</th>
    <td><%= event.title %></td>
  </tr>
  <tr>
    <th>Description</th>
    <td><%= event.description %></td>
  </tr>
  <tr>
    <th>Location</th>
    <td><%= event.location %></td>
  </tr>
  <tr>
    <th>Start Date</th>
    <td><%= event.startDate %></td>
  </tr>
  <tr>
    <th>End Date</th>
    <td><%= event.endDate %></td>
  </tr>
  <tr>
    <th>Is Online?</th>
    <td><%= event.isOnline %></td>
  </tr>
  <tr>
    <th>Registration Link</th>
    <td><%= event.registrationLink %></td>
  </tr>
  <tr>
    <th>Organizer</th>
    <td><%= event.organizer %></td>
  </tr>
  <tr>
    <th>Attendees</th>
    <td><%= event.attendees %></td>
  </tr>
  
 
</table>
<% if (currentUser) { %>
  <div>
    <a href="<%= `/events/${event._id}/${currentUser._id}/attend` %>"
      >Attend</a
    >
    </div>
  <% } else { %>
  <div>
    <a href="<%= `/events/${event._id}/attend` %>">Attend</a>
  </div>
  <% } %>
<div>
  <a href="/events">View all events</a>
</div>
<div>
  <a
    href="<%= `/events/${event._id}/delete?_method=DELETE` %>"
    onclick="return confirm('Are you sure you want to delete this record?')"
    >Delete</a
  >
</div>

